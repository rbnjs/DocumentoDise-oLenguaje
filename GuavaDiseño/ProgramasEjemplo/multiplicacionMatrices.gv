/*
 * Multiplicacion de matrices en Guava.
 */
function void main(){
    declare{
        integer array a[3][3],b[3][3],c[3][3];
        integer var i,j,tmp;
    }
    a := [[1,2,3],
          [4,5,6],
          [7,8,9]];

    c := [[0,0,0],
          [0,0,0],
          [0,0,0]];

    b := a;
    i := 0;
    while ( i < 3) do {
        j := 0;
        while ( j < 3) do {
            c[i][j] := c[i][j] + (a[i][j]*b[j][i]);
            j++;
        }
        i++;
    }
}
